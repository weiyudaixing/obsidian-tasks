{
    "npm.packageManager": "yarn",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/*.lock": true,
        "**/*config*": true,
        "**/*.sh": true,
        "**/*report*": true,
        "**/*.md": true,
        "**/*.svg": true,
        "**/*.css": true,
        "**/.github/": true,
        "**/contributing/": true,
        "**/docs/": true,
        "**/resources/": true,
        "**/*uery.ts": true,
        "**/*.ps1": true,
        "src/Obsidian/": true,
        "src/Config/": true,
        "src/Api/": true,
        "src/DateTime/": true,
        "src/lib/": true,
        "src/Commands/": true,
        "src/main.ts": true,
        "*.eslintrc.js": true,
        "package.json": true,
        "src/Query/": true,
        "tests/lib/": true,
        "**/*.yml": true,
    },
    "markdownlint.config": {},
    "css.format.enable": false,
    "markdown.extension.tableFormatter.enabled": false,
    "markdown.extension.tableFormatter.delimiterRowNoPadding": false,
    "json.format.keepLines": true,
    "json.format.enable": false,
    "files.trimTrailingWhitespaceInRegexAndStrings": false,
    "less.format.enable": false,
    "scss.format.enable": false,
    "markdown.editor.pasteUrlAsFormattedLink.enabled": "never",
    "editor.formatOnSave": false,
    "markdown.preview.breaks": false,
    "html.format.preserveNewLines": false,
    "html.format.enable": false
}
